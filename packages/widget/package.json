{
  "name": "@pouw/widget",
  "version": "0.1.0",
  "description": "PoUW CAPTCHA Widget - Browser-based ML CAPTCHA",
  "main": "dist/pouw-captcha.umd.js",
  "module": "dist/pouw-captcha.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/pouw-captcha.umd.js",
  "jsdelivr": "dist/pouw-captcha.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "onnxruntime-web": "^1.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.0.4",
    "jsdom": "^23.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4"
  },
  "peerDependencies": {},
  "keywords": [
    "captcha",
    "machine-learning",
    "tensorflow",
    "onnx",
    "proof-of-work",
    "security",
    "accessibility"
  ],
  "author": "PoUW Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pouw-captcha.git",
    "directory": "packages/widget"
  }
}
